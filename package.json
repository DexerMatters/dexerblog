{
  "name": "dexerblog-workspace",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm --filter dexerblog-frontend dev",
    "dev:frontend": "pnpm --filter dexerblog-frontend dev",
    "dev:backend": "pnpm --filter dexerblog-backend dev",
    "build": "pnpm --filter dexerblog-frontend build && pnpm --filter dexerblog-backend build",
    "build:frontend": "pnpm --filter dexerblog-frontend build",
    "build:backend": "pnpm --filter dexerblog-backend build",
    "start": "pnpm --filter dexerblog-frontend start",
    "start:frontend": "pnpm --filter dexerblog-frontend start",
    "start:backend": "pnpm --filter dexerblog-backend start",
    "lint": "pnpm --filter dexerblog-frontend lint",
    "install:all": "pnpm install"
  },
  "devDependencies": {
    "@types/node": "^20",
    "typescript": "^5"
  },
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad",
  "engines": {
    "node": ">=18.0.0"
  }
}